<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="testResultDAO" class="com.griddynamics.testresult.dao.impl.JdbcTestResultDAO">
        <property name="dataSource" ref="dataSourceSQLite" />
    </bean>

    <bean id="testAreaDAO" class="com.griddynamics.testresult.dao.impl.JdbcTestAreaDAO">
        <property name="dataSource" ref="dataSourceSQLite" />
    </bean>

</beans>